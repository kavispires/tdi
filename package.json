{
  "name": "tdi",
  "version": "1.0.0",
  "homepage": "http://www.kavispires.com/tdi/",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/lodash": "^4.14.191",
    "@types/node": "^17.0.41",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "5.3.3",
    "antd": "^5.1.5",
    "clsx": "^1.2.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.2.1",
    "react-scripts": "4.0.3",
    "react-use": "^17.4.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "printWidth": 110,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "overrides": [
      {
        "files": "/functions/*.js",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3"
  }
}
